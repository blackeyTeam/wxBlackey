page {
  background: #f5f6f7;
}
.head-nav {
  width: 100%;
  height: 100rpx;
  font-size: 0;
  background-color: #fff;
}

.head-nav .txt {
  height: 80rpx;
  line-height: 100rpx;
  text-align: center;
  display: inline-block;
  margin: 0 30rpx;
  font-size: 30rpx;
}

.head-nav .selected {
  border-bottom: 6rpx solid #1296db;
  font-weight: bold;
}